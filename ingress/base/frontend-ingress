apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
  name: privideo-frontend-ingress
  # rewrite 없음!
spec:
  ingressClassName: nginx
  rules:
  - http:
      paths:
      - path: /
        pathType: Prefix
        backend:
          service:
            name: privideo-frontend-service
            port:
              number: 80
